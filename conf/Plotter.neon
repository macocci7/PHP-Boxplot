fontPath: 'fonts/ipaexg.ttf' # IPA ex Gothic 00401
#fontPath: 'fonts/ipaexm.ttf' # IPA ex Mincho 00401
fontSize: 16
fontColor: '#333333'
outlier: true
outlierDiameter: 2
outlierColor: '#ff0000'
jitter: false
jitterColor: '#009900'
