# Configurantion for Macocci7\PhpBoxplot\Plotter

canvasWidth: 600
canvasHeight: 500
canvasBackgroundColor: '#ffffff'
plotarea:
    backgroundColor:
frameXRatio: 0.8
frameYRatio: 0.7
showAxis: true
axisColor: '#666666'
axisWidth: 1
gridColor: '#999999'
